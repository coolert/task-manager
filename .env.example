# This configuration works out-of-the-box with Laravel Sail (MySQL / Redis / RabbitMQ).
# If not using Sail, adjust DB_* / REDIS_* / RABBITMQ_* accordingly.
APP_NAME=TaskManager
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=sail
DB_PASSWORD=password
MYSQL_EXTRA_OPTIONS=

QUEUE_CONNECTION=redis

CACHE_STORE=redis
# CACHE_PREFIX=

REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

WWWUSER=1000
WWWGROUP=1000

SEED_PROFILE=demo

JWT_SECRET=
JWT_ALGO=HS256

RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_VHOST=/
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_PREFETCH_COUNT=1
RABBITMQ_QUEUE=default

# Optional demo webhook endpoint for message pipeline examples
TASK_CREATED_WEBHOOK_URL=https://httpbin.org/post
